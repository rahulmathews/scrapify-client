{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "no-console": "warn",
        "prettier/prettier": ["error"],
        "no-alert": "warn",
        "no-undefined": "error",
        "no-var": "error",
        "dot-notation": "warn",
        "no-unneeded-ternary": 2,
        "no-unused-expressions": 2
    }
}
